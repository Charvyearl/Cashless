{
  "cli": {
    "version": ">= 16.21.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_CASHLESS_API_URL": "http://192.168.1.22:3000"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_CASHLESS_API_URL": "https://YOUR-PROD-API"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
